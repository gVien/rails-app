<% if @feed_items.any? %>
  <ol class="microposts">
    <%# rails magic: rendering feed items will cause rails to look for a micropost partial (since each item in feed items has class Micropost) and this will be _micrposts.html.erb  %>
    <%= render @feed_items %>
  </ol>
  <%= will_paginate @feed_items %>
<% end %>